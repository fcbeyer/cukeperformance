<div class="modal-header">
	<button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
    <h3 id="myModalLabel">Data Retrieval Form</h3>
</div>
<div class="modal-body">
	<%= form_tag 'none', :id => 'graphParameterData' do %>
		<fieldset>
			<%= label_tag :begin_date, "Begin Date: " %>
			<%= begin_date_display %>
			
			<%= label_tag :end_date, "End Date: " %>
			<%= end_date_display %>
			
			<%= label_tag "Select a Suite" %>
			<%= select_suite_display  %>
		</fieldset>
	<% end %>
</div>
<div class="modal-footer">
  <button class="btn btn-large btn-block btn-success" data-dismiss="modal" onclick="getData('Suites',drawSuitesGraphs)" type="button">Make It So!</button>
</div>
