<ul class="nav nav-tabs">
  <% @suite_list.each do |suite| %>
  	<li><a href="#<%= suite.name %>" data-toggle="tab"><%= suite.display_name %> Builds</a></li>
  <% end %>
</ul>

<div class="tab-content">
	<% @suite_list.each do |entry| %>
		<div class="tab-pane" id="<%= entry.name %>">
			<table class="table table-bordered table-condensed">
			  <tr>
				<th>Build Timestamp</th>
			    <th>Duration</th>
			    <th>Name</th>
			    <th>Status</th>
			    <th></th>
			    <th></th>
			  </tr>
			
			<% @suites[entry.name].each do |suite| %>
			  <tr>
			    <td><%= suite.runstamp %></td>
			    <td><%= suite.duration_converted %></td>
			    <td><%= suite.name %></td>
			    <% if suite.status == "failed" %>
			    	<td><span class="badge badge-warning"><%= suite.status %></span></td>
			    <% else %>
			    	<td><span class="label label-inverse"><%= suite.status %></span></td>
			    <% end %>
			    <td><%= link_to 'Show', suite %></td>
			    <td><%= link_to 'Destroy', suite, confirm: 'Are you sure?', method: :delete %></td>
			  </tr>
			<% end %>
			</table>
		</div>
	<% end %>
