<p id="notice"><%= notice %></p>
<div class="container-fluid">
	<div class="row-fluid">
		<div class="span12">
			<table class="table table-bordered table-hover">
			  <tr>
			    <th>Build Timestamp</th>
			    <th>Duration</th>
			    <th>Name</th>
			    <th>Browser</th>
			    <th>OS</th>
			    <th>Mobilizer</th>
			    <th>Mobilizer Build Tag</th>
			    <th>URL</th>
			    <th>Exclude?</th>
			    <th></th>
			    <th></th>
			  </tr>
			  <tr>
			    <td><%= @current_suite.runstamp %></td>
			    <td><%= @current_suite.duration_converted %></td>
			    <td><%= @current_suite.name %></td>
			    <td><%= @current_suite.browser %></td>
			    <td><%= @current_suite.os %></td>
			    <td><%= @current_suite.mobilizer %></td>
			    <td><%= @current_suite.mobilizer_build_tag %></td>
			    <td><%= @current_suite.url %></td>
			    <td><%= active_flag_display(@current_suite.exclude) %></td>
			    <td><%= link_to 'Edit', edit_suite_path(@current_suite), :class => "btn btn-small"  %></td>
			    <td><%= link_to 'Destroy', @current_suite, confirm: 'Are you sure?', method: :delete, :class => "btn btn-small"  %></td>
			  </tr>
			</table>
			
			
			<%= render 'features/put_features' %>
			
			<br />
			
			<%= link_to 'Back', suites_path, :class => "btn btn-small"  %>
		</div>
	</div>
</div>
