<p id="notice"><%= notice %></p>
<div class="container-fluid">
	<div class="row-fluid">
		<div class="span12">
			<ul class="breadcrumb">
			  <li><%= link_to @current_suite.name, get_tab_path(suites_path,@current_suite.name) %><span class="divider">/</span></li>
			  <li class="active"><%= time_tag(@current_suite.runstamp,:format=>'%B %d, %Y %l:%M %p') %></li>
			</ul>
			<table class="table table-bordered table-hover">
			  <tr>
			    <th>Build Timestamp</th>
			    <th>Duration</th>
			    <th>Name</th>
			    <th>Browser</th>
			    <th>OS</th>
			    <th>Mobilizer</th>
			    <th>Mobilizer Build Tag</th>
			    <th>URL</th>
			    <th>Exclude?</th>
			    <th></th>
			    <th></th>
			  </tr>
			  <tr>
			    <td><%= time_tag(@current_suite.runstamp,:format=>'%B %d, %Y %l:%M %p') %></td>
			    <td><%= @current_suite.duration_converted %></td>
			    <td><%= @current_suite.name %></td>
			    <td><%= @current_suite.browser %></td>
			    <td><%= @current_suite.os %></td>
			    <td><%= @current_suite.mobilizer %></td>
			    <td><%= @current_suite.mobilizer_build_tag %></td>
			    <td><%= @current_suite.url %></td>
			    <td><%= active_flag_display(@current_suite.exclude) %></td>
			    <td><%= link_to 'Edit', edit_suite_path(@current_suite), :class => "btn btn-small"  %></td>
			    <td><%= link_to 'Destroy', @current_suite, confirm: 'Are you sure?', method: :delete, :class => "btn btn-small"  %></td>
			  </tr>
			</table>
			<div class="container-fluid">
				<div class="row-fluid">
					<div class="span12">
						<%= render 'features/put_features' %>
					</div>
				</div>
			</div>
			<!-- <%= link_to 'Back', suites_path, :class => "btn btn-small"  %> -->
		</div>
	</div>
</div>
